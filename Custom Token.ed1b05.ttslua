scoreTilePieces = {'ece6a7', '846f06', '19f26f', 'f12fb7', '04f4be', 'f698df'}
playerScoresByIndex = {0, 0, 0, 0, 0, 0}

function getPlayerColorToIndex(color)
    playerColorToIndex = 0

    if(color == 'Red' or color == Color.Red) then playerColorToIndex = 1 end
    if(color == 'Orange' or color == Color.Orange) then playerColorToIndex = 2 end
    if(color == 'Yellow' or color == Color.Yellow) then playerColorToIndex = 3 end
    if(color == 'Purple' or color == Color.Purple) then playerColorToIndex = 4 end
    if(color == 'Blue' or color == Color.Blue) then playerColorToIndex = 5 end
    if(color == 'Green' or color == Color.Green) then playerColorToIndex = 6 end
    
    return playerColorToIndex
end

scoreTable = {1, 2, 4, 7, 7, 15, 15, 21}
snapPoints = Global.getSnapPoints()

function moveScoreCounterOnBoard(route, color, oper)
    routeLength = #route
    playerIndex = getPlayerColorToIndex(color)
    if oper == "add" then
        playerScoresByIndex[playerIndex] = playerScoresByIndex[playerIndex] + scoreTable[routeLength]
    elseif oper == "sub" then
        playerScoresByIndex[playerIndex] = playerScoresByIndex[playerIndex] - scoreTable[routeLength]
    end
    if playerScoresByIndex[playerIndex] >= 80 then
        playerScoresByIndex[playerIndex] = playerScoresByIndex[playerIndex] - 80
    end
    for i, v in ipairs(snapPoints) do
        if i > 80 then return nil end -- This means something broke, there's only 80 scoring snapoints.
        if i == playerScoresByIndex[playerIndex] then
            x,y,z = v.position.x, v.position.y, v.position.z
            getObjectFromGUID(scoreTilePieces[playerIndex]).setPositionSmooth(Vector(x, y+4, z))
            return true
        end
    end
end

function getTurnCounter()
    return getObjectFromGUID('0b2ed0').getVar('turnCounter')
end

function setTurnCounter(number)
    getObjectFromGUID('0b2ed0').setVar('turnCounter', number)
    return nil
end

function isPlayerTurnOrPromoted(color)
    table = getSeatedPlayers()
    for _, value in pairs(table) do
        if value == color and Turns.turn_color == color then
            return true
        elseif Player[color].admin == true then
            return true
        end
    end
    return false
end

function placeTrainRoute(route, color, alt_click)
    if isPlayerTurnOrPromoted(color) then
        if alt_click == true then
            originalRouteColor = hideTrainRoute(route)
            if getObjectFromGUID(route[1]).getColorTint() ~= "White" and (Player[color].admin ~= true or Turns.turn_color == color) then
                if getTurnCounter() == 3 and not (getTurnCounter() <= 2) then
                    increaseTrainCounter(route, originalRouteColor)
                    moveScoreCounterOnBoard(route, color, "sub")
                    setTurnCounter(0)
                    broadcastToAll(Player[color].steam_name .. " has undone a train route of length " .. #route)
                    return nil
                elseif Player[color].admin == true then
                    increaseTrainCounter(route, originalRouteColor)
                    return nil
                else
                    broadcastToColor("It's not your turn or you are not an admin so you cannot undo this section.", color, errColor)
                end
                return nil
            end
        end
        if getTurnCounter() >= 1 then
            broadcastToColor("You have already taken another action this turn, please complete your action or end your turn, or right click the route you placed to undo it.", color, errColor)
            return nil
        end
        if showTrainRoute(route, color, alt_click) then
            reduceTrainCounter(route, color)
            setTurnCounter(3)
            moveScoreCounterOnBoard(route, color, "add") -- ToDo: Build index table of all possible scores and pass #route to this variable using that table.
        end
    else
        broadcastToColor("It's not your turn or you are not seated in a valid colour.", color, errColor)
    end
end

function hideTrainRoute(route)
    originalRouteColor = getObjectFromGUID(route[1]).getColorTint()
    for i, v in ipairs(route) do
        obj = getObjectFromGUID(v)
        obj.setColorTint('White')
        obj.setInvisibleTo({'Orange', 'Red', 'Green', 'Blue', 'Purple', 'Yellow', 'Teal', 'Brown', 'Pink', 'White', 'Grey', 'Black'})
    end
    return originalRouteColor
end

function showTrainRoute(route, color, alt_click)
    if alt_click then
        return nil
    end
    for i, v in ipairs(route) do
        obj = getObjectFromGUID(v)
        if obj.getColorTint() ~= Color.White then
            broadcastToColor(Player[color].steam_name .. " - you can't place trains where another player's trains are.", color, stringColorToRGB("Teal"))
            return nil
        end
        obj.setColorTint(color)
        position = obj.getPosition()
        x, y, z = position:get()
        obj.setPositionSmooth(Vector(x, y+1, z))
        obj.setLock(false)
        obj.setInvisibleTo({})
    end
    -- Waits 180 frames for gravity to take effect, and then locks trains so buttons underneath trains can be clicked more easily.
    Wait.frames(function() for i, v in ipairs(route) do obj = getObjectFromGUID(v).setLock(true) end end, 180)
    return true
end

function reduceTrainCounter(route, color)
    playerColorToIndex = getPlayerColorToIndex(color)

    trainCounter = getObjectFromGUID(allTrainCounters[playerColorToIndex])
    currentCount = trainCounter.getValue()
    currentCount = trainCounter.setValue(currentCount - #route)
end

function increaseTrainCounter(route, color)
    playerColorToIndex = getPlayerColorToIndex(color)

    if playerColorToIndex == 0 then
        return nil
    end

    trainCounter = getObjectFromGUID(allTrainCounters[playerColorToIndex])
    currentCount = trainCounter.getValue()
    
    currentCount = trainCounter.setValue(currentCount + #route)
end

function map_Cadiz_to_Madrid(obj, color, alt_click)
    placeTrainRoute(Cadiz_to_Madrid, color, alt_click)
end
