function onLoad()
    maxTrainsPerPlayer = 45
    for i, v in ipairs(allTrainCounters) do
        obj = getObjectFromGUID(v)
        obj.setValue(maxTrainsPerPlayer)
    end

    -- Hide all train routes initially
    for i, v in ipairs(AllRoutes) do
        hideTrainRoute(v)
    end
    -- Lock all trains as players do NOT need to interact with them
